for row in range(7):
	for col in range(5):
		print('* ',end=' ')
	print()

for row in range(7):
	for col in range(5,0,2):
		if (col ==0 or col==4):
			print('* ',end=' ')
		else:
			print(' ',end =' ')
	print()	
	
for i in range(7):
		for j in range(5):
			if (i==0 or i==6):
				print('* ',end=' ')
			elif (j==0 or j==4):
				print('* ',end=' ')
			else:
				print(' ',end=' ')
		print()		
				
	
	
	